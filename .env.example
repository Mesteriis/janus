# Dashboard
DASHBOARD_PORT=8090
FEATURE_TUNNEL_ENABLED=true
FEATURE_VPN_ENABLED=true
SETTINGS_JSON_FILE=data/settings/app_settings.json

# Cloudflare Tunnel
CLOUDFLARE_TUNNEL_TOKEN=your-tunnel-token
CLOUDFLARE_API_TOKEN=your-api-token
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_TUNNEL_ID=your-tunnel-id
# Для Docker Desktop/bridge сети используйте http://caddy:80
# Для host-сети (Linux) используйте http://127.0.0.1:80
CLOUDFLARE_DEFAULT_SERVICE=http://caddy:80
CADDY_RUNTIME_HTTP_PORT=80
CADDY_RUNTIME_HTTPS_PORT=443
CLOUDFLARE_HOSTNAMES_FILE=data/cloudflare/hostnames.json
CLOUDFLARE_STATE_FILE=data/cloudflare/state.json

# Cloudflare DNS (dns-01)
CF_API_TOKEN=

# TLS redis storage
TLS_REDIS_USERNAME=
TLS_REDIS_PASSWORD=
